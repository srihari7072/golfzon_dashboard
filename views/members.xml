<odoo>
    <!-- Tree View -->
    <record id="view_members_tree" model="ir.ui.view">
        <field name="name">members.members.tree</field>
        <field name="model">members.members</field>
        <field name="arch" type="xml">
            <list string="Members">
                <field name="member_id"/>
                <field name="person_code"/>
                <field name="account_id"/>
                <field name="member_no"/>
                <field name="member_class_cd_id"/>
                <field name="member_cd_id"/>
                <field name="corp_scd"/>
                <field name="acquisition_cd_id"/>
                <field name="acquisition_amt"/>
                <field name="company_name"/>
              
            </list>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_members_form" model="ir.ui.view">
        <field name="name">members.members.form</field>
        <field name="model">members.members</field>
        <field name="arch" type="xml">
                        <form string="Members">
                <sheet>
                    <group>
                        <group>
                            <field name="member_id"/>
                            <field name="person_code"/>
                            <field name="account_id"/>
                            <field name="member_no"/>
                            <field name="member_class_cd_id"/>
                            <field name="member_cd_id"/>
                            <field name="corp_scd"/>
                            <field name="acquisition_cd_id"/>
                            <field name="acquisition_amt"/>
                            <field name="company_name"/>
                            <field name="bsu_no"/>
                            <field name="innumber"/>
                            <field name="entry_cd_id"/>
                            <field name="entry_date"/>
                            <field name="entry_amt"/>
                            <field name="due_date"/>
                            <field name="benefits_gpclass_scd"/>
                            <field name="benefits_class_code"/>
                            <field name="restrict_yn"/>
                            <field name="acquisition_date"/>
                            <field name="remark"/>
                        </group>
                        <group>
                            <field name="created_by"/>
                            <field name="created_at"/>
                            <field name="updated_by"/>
                            <field name="updated_at"/>
                            <field name="deleted_by"/>
                            <field name="deleted_at"/>
                            <field name="ref_member_by"/>
                        </group>
                    </group>
                </sheet>
            </form>

        </field>
    </record>

    <!-- Action -->
    <record id="action_members" model="ir.actions.act_window">
        <field name="name">Members</field>
        <field name="res_model">members.members</field>
        <field name="view_mode">list,form</field>
    </record>

    <!-- Menu -->
    <menuitem id="menu_members_root" name="Members"/>
    <menuitem id="menu_members" name="Records"
              parent="menu_members_root"
              action="action_members"/>
</odoo>
