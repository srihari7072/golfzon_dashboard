<odoo>
    <!-- List View -->
    <record id="view_admin_login_his_list" model="ir.ui.view">
        <field name="name">admin.login.his.list</field>
        <field name="model">admin_login_his</field>
        <field name="arch" type="xml">
            <list>
                <field name="admin_code"/>
                <field name="account_id"/>
                <field name="access_ip"/>
                <field name="login_at"/>
                <field name="logout_at"/>
            </list>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_admin_login_his_form" model="ir.ui.view">
        <field name="name">admin.login.his.form</field>
        <field name="model">admin_login_his</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="admin_code"/>
                        <field name="account_id"/>
                        <field name="access_ip"/>
                        <field name="login_at"/>
                        <field name="logout_at"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Action -->
    <record id="action_admin_login_his" model="ir.actions.act_window">
        <field name="name">Admin Login History</field>
        <field name="res_model">admin_login_his</field>
        <field name="view_mode">list,form</field>
    </record>

    <!-- Menu -->
    <menuitem id="menu_admin_login_his_root" name="admin_login_his"/>
    <menuitem id="menu_admin_login_his" name="Login History"
              parent="menu_admin_login_his_root" action="action_admin_login_his"/>
</odoo>
