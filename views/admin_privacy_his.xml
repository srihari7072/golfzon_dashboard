<odoo>
    <!-- List View -->
    <record id="view_admin_privacy_his_list" model="ir.ui.view">
        <field name="name">admin.privacy.his.list</field>
        <field name="model">admin.privacy.his</field>
        <field name="arch" type="xml">
            <list string="Admin Privacy History">
                <field name="admin_privacy_his_id"/>
                <field name="person_code"/>
                <field name="admin_code"/>
                <field name="modify_scd"/>
                <field name="before_value"/>
                <field name="after_value"/>
                <field name="program_group_code"/>
                <field name="client_ip"/>
                <field name="Account_id"/>
                <field name="created_at"/>
                <field name="process_cnt"/>
            </list>
        </field>
    </record>

    <!-- Form View -->
    <record id="view_admin_privacy_his_form" model="ir.ui.view">
        <field name="name">admin.privacy.his.form</field>
        <field name="model">admin.privacy.his</field>
        <field name="arch" type="xml">
            <form string="Admin Privacy History">
                <sheet>
                    <group>
                        <field name="admin_privacy_his_id"/>
                        <field name="person_code"/>
                        <field name="admin_code"/>
                        <field name="modify_scd"/>
                        <field name="before_value"/>
                        <field name="after_value"/>
                        <field name="program_group_code"/>
                        <field name="client_ip"/>
                        <field name="Account_id"/>
                        <field name="created_at"/>
                        <field name="process_cnt"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Action -->
    <record id="action_admin_privacy_his" model="ir.actions.act_window">
        <field name="name">Admin Privacy History</field>
        <field name="res_model">admin.privacy.his</field>
        <field name="view_mode">list,form</field>
    </record>

    <!-- Menus -->
    <!-- Main Root Menu (appears in top bar) -->
    <menuitem id="menu_admin_privacy_his_root" name="Admin_privacy_his"/>

    <!-- Submenu linked to action -->
    <menuitem id="menu_admin_privacy_his_sub"
              name="Privacy History"
              parent="menu_admin_privacy_his_root"
              action="action_admin_privacy_his"/>
</odoo>
